
#example how the config file should be like:
#server1
server     #hello  

{
	      listen		1;
		listen 20;
	host      /zasabri;
	     server_name hi by say tay;     ;;; 			            
	limit_client_body          10;
	      error_page     301   /home/zasabri/Desktop/errors;
	      error_page      304  /home/zasabri/Desktop/errorsat;
location   / {
		root	/var/www/html  	 ; 		  #hi 
		index	index.html index.php index.py; 
			allowed_methodes GET	  POST  DELETE;
		autoindex       on;  
return 301 	www.zasabri.ma;
					upload	on | off /var/www/html;
			cgi on | off adress:port;  
	}   
}        
#server2
