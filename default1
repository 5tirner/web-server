server  {
    listen       8080;       
    
    
    
    server_name _;
    location /test
    {
        root /var/www/html;
        index index.html index.htm index.nginx-debian.html;
    }
}

server          {
        listen 8080;

        server_name _;
        location /test
        {
            root /var/www/html;

            index index.html index.htm index.nginx-debian.html;
        }
}